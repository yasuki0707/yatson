{
  "name": "yatson",
  "version": "1.0.0",
  "description": "convert audio data to text, then process that text",
  "main": "index.js",
  "dependencies": {
    "@types/json2csv": "^5.0.3",
    "dotenv": "^10.0.0",
    "fs": "0.0.1-security",
    "ibm-watson": "^6.1.1",
    "iconv-lite": "^0.6.3",
    "json2csv": "^5.0.6",
    "node-fetch": "^2.6.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.45",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "0.0.9"
  },
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "tsc": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build": "npm-run-all clean tsc",
    "start": "node .",
    "eslint": "eslint src/index.ts",
    "format": "prettier --write '**/*.{js,ts,json}'",
    "test:ut": "jest",
    "test:it": "npm run tsc; sh/test.sh",
    "test:it:ok": "npm run test:it ./resources/audio/public_audio_ja-JP_Narrowband-sample.wav 山田",
    "test:it:ng1": "npm run test:it",
    "test:it:ng2": "npm run test:it ./resources/audio/public_audio_ja-JP_Narrowband-sample.wav",
    "test:it:ng3": "npm run test:it 山田",
    "test:it:ng4": "npm run test:it ./resources/audio/this_is_not_audio 山田",
    "tool:audio": "ts-node -r tsconfig-paths/register ./src/tools/CheckAudioLocaleTool.ts",
    "tool:audio:ja": "npm run tool:audio ja_JP",
    "tool:audio:en": "npm run tool:audio en_US",
    "tool:audio:size": "ts-node -r tsconfig-paths/register ./src/tools/CheckAudioSizeTool.ts"
  },
  "author": "yasu",
  "license": "ISC"
}
